{
  "name": "ineslozanopalacio",
  "version": "1.0.0",
  "description": "personal site for ines lozano palacio",
  "main": "index.js",
  "scripts": {
    "bundle": "rollup -c",
    "bundle:watch": "rollup -c -w",
    "template": "node buildPug.js",
    "template:watch": "node buildPug.js --watch",
    "build:dev": "npx run-p **:watch",
    "build": "npx run-s clean:build bundle template",
    "start": "sirv public",
    "clean:build": "npx rimraf public/build public/*.html",
    "clean:deps": "npx rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:deps",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crellison/ineslozanopalacio.git"
  },
  "author": "Cole Ellison (cole.ellison@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crellison/ineslozanopalacio/issues"
  },
  "homepage": "https://github.com/crellison/ineslozanopalacio#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "marked": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^6.1.0",
    "sirv-cli": "^0.4.6",
    "svelte": "^3.22.3"
  }
}
